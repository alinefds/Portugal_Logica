algoritmo "Exercicio_10"

var

altura, largura, area, litro: real

inicio

escreva("Digite a largura da parede: ")
leia(largura)
escreva("Digite a altura da parede: ")
leia(altura)
area <- largura * altura
litro <- area / 2
escreva("A quantidade de tinta necessaria para o servico e de:", litro, " litro(s)")


fimalgoritmo
