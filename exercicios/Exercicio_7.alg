algoritmo "Exercicio 7"
var
   num, dobro, terca_parte: real

inicio
      Escreva("Digite um numero: ")
      Leia(num)
      dobro <- num * 2
      terca_parte <- num /3
      Escreval("O numero que voce digitou foi:", num)
      Escreval("E o dobro de ", num, " e", dobro)
      Escreval("A terca parte de ", num, " e", terca_parte)
      
// Seção de Comandos 
fimalgoritmo
